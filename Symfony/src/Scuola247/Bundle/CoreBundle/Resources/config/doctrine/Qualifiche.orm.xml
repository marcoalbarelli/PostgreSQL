<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Scuola247\Bundle\CoreBundle\Entity\Qualifiche" table="qualifiche">
    <id name="qualifica" type="integer" column="qualifica">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="annodicorso" type="integer" column="annodicorso" nullable="true"/>
    <field name="nome" type="string" column="nome" length="160" nullable="false"/>
    <field name="descrizione" type="string" column="descrizione" length="4000" nullable="false"/>
    <field name="tipo" type="string" column="tipo" nullable="false"/>
    <many-to-one field="metrica" target-entity="Metriche">
      <join-columns>
        <join-column name="metrica" referenced-column-name="metrica"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="materia" target-entity="Materie">
      <join-columns>
        <join-column name="materia" referenced-column-name="materia"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="istituto" target-entity="Istituti">
      <join-columns>
        <join-column name="istituto" referenced-column-name="istituto"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="indirizzoscolastico" target-entity="Indirizziscolastici">
      <join-columns>
        <join-column name="indirizzoscolastico" referenced-column-name="indirizzoscolastico"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
