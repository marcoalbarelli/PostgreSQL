<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Scuola247\Bundle\CoreBundle\Entity\Personegiuridiche" table="personegiuridiche">
    <field name="partitaiva" type="string" column="partitaiva" length="11" nullable="true"/>
    <field name="codicefiscale" type="string" column="codicefiscale" length="16" nullable="true"/>
    <one-to-one field="personagiuridica" target-entity="Soggetti">
      <join-columns>
        <join-column name="personagiuridica" referenced-column-name="soggetto"/>
      </join-columns>
    </one-to-one>
    <many-to-one field="tipopersonagiuridica" target-entity="Tipipersonegiuridiche">
      <join-columns>
        <join-column name="tipopersonagiuridica" referenced-column-name="tipopersonagiuridica"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="nazione" target-entity="Nazioni">
      <join-columns>
        <join-column name="nazione" referenced-column-name="nazione"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
