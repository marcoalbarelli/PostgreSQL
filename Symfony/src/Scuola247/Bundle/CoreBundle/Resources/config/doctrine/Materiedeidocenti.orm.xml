<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Scuola247\Bundle\CoreBundle\Entity\Materiedeidocenti" table="materiedeidocenti">
    <id name="materiadeldocente" type="integer" column="materiadeldocente">
      <generator strategy="SEQUENCE"/>
    </id>
    <many-to-one field="docente" target-entity="Personefisiche">
      <join-columns>
        <join-column name="docente" referenced-column-name="personafisica"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="materia" target-entity="Materie">
      <join-columns>
        <join-column name="materia" referenced-column-name="materia"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="classe" target-entity="Classi">
      <join-columns>
        <join-column name="classe" referenced-column-name="classe"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
