<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Scuola247\Bundle\CoreBundle\Entity\Figureprofessionalidettagli" table="figureprofessionalidettagli">
    <id name="figuraprofessionaledettaglio" type="integer" column="figuraprofessionaledettaglio">
      <generator strategy="SEQUENCE"/>
    </id>
    <many-to-one field="soggetto" target-entity="Soggetti">
      <join-columns>
        <join-column name="soggetto" referenced-column-name="soggetto"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="personafisica" target-entity="Personefisiche">
      <join-columns>
        <join-column name="personafisica" referenced-column-name="personafisica"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="figuraprofessionale" target-entity="Figureprofessionali">
      <join-columns>
        <join-column name="figuraprofessionale" referenced-column-name="figuraprofessionale"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
