<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Scuola247\Bundle\CoreBundle\Entity\Mezzidicomunicazione" table="mezzidicomunicazione">
    <id name="mezzodicomunicazione" type="integer" column="mezzodicomunicazione">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="descrizione" type="string" column="descrizione" length="160" nullable="true"/>
    <field name="percorso" type="string" column="percorso" length="255" nullable="false"/>
    <many-to-one field="tipodicomunicazione" target-entity="Tipidicomunicazione">
      <join-columns>
        <join-column name="tipodicomunicazione" referenced-column-name="tipodicomunicazione"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="soggetto" target-entity="Soggetti">
      <join-columns>
        <join-column name="soggetto" referenced-column-name="soggetto"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
