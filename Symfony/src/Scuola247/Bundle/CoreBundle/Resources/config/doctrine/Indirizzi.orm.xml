<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Scuola247\Bundle\CoreBundle\Entity\Indirizzi" table="indirizzi">
    <id name="indirizzo" type="integer" column="indirizzo">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="prefissovia" type="string" column="prefissovia" length="15" nullable="true"/>
    <field name="via" type="string" column="via" length="160" nullable="true"/>
    <field name="civico" type="string" column="civico" length="15" nullable="true"/>
    <field name="isolato" type="string" column="isolato" length="60" nullable="true"/>
    <field name="palazzo" type="string" column="palazzo" length="60" nullable="true"/>
    <field name="scala" type="string" column="scala" length="60" nullable="true"/>
    <field name="piano" type="string" column="piano" length="15" nullable="true"/>
    <field name="interno" type="string" column="interno" length="15" nullable="true"/>
    <field name="cap" type="string" column="cap" length="15" nullable="true"/>
    <field name="localita" type="string" column="localita" length="160" nullable="true"/>
    <field name="provincia" type="string" column="provincia" length="160" nullable="true"/>
    <many-to-one field="tipoindirizzo" target-entity="Tipiindirizzi">
      <join-columns>
        <join-column name="tipoindirizzo" referenced-column-name="tipoindirizzo"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="soggetto" target-entity="Soggetti">
      <join-columns>
        <join-column name="soggetto" referenced-column-name="soggetto"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="nazione" target-entity="Nazioni">
      <join-columns>
        <join-column name="nazione" referenced-column-name="nazione"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
