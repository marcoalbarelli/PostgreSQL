<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Scuola247\Bundle\CoreBundle\Entity\Logfirme" table="logfirme">
    <id name="logfirma" type="integer" column="logfirma">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="logtimestamp" type="string" column="logtimestamp" nullable="false"/>
    <field name="giorno" type="date" column="giorno" nullable="false"/>
    <field name="periododilezione" type="integer" column="periododilezione" nullable="false"/>
    <many-to-one field="logrevisore" target-entity="Personefisiche">
      <join-columns>
        <join-column name="logrevisore" referenced-column-name="personafisica"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="docente" target-entity="Personefisiche">
      <join-columns>
        <join-column name="docente" referenced-column-name="personafisica"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="firma" target-entity="Firme">
      <join-columns>
        <join-column name="firma" referenced-column-name="firma"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="classe" target-entity="Classi">
      <join-columns>
        <join-column name="classe" referenced-column-name="classe"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
