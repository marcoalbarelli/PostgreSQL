{% extends '::base.html.twig' %}

{% block body -%}
    <h1>Soggetti</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Descrizione</th>
                <td>{{ entity.descrizione }}</td>
            </tr>
            <tr>
                <th>Tiposoggetto</th>
                <td>{{ entity.tiposoggetto }}</td>
            </tr>
            <tr>
                <th>Foto</th>
                <td>{{ entity.foto }}</td>
            </tr>
            <tr>
                <th>Autorizzazionedatipersonali</th>
                <td>{{ entity.autorizzazionedatipersonali|date('Y-m-d H:i:s') }}</td>
            </tr>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
        </tbody>
    </table>

        <ul class="record_actions">
    <li>
        <a href="{{ path('soggetti') }}">
            Back to the list
        </a>
    </li>
    <li>
        <a href="{{ path('soggetti_edit', { 'id': entity.id }) }}">
            Edit
        </a>
    </li>
    <li>
        <form action="{{ path('soggetti_delete', { 'id': entity.id }) }}" method="post">
            <input type="hidden" name="_method" value="DELETE" />
            {{ form_widget(delete_form) }}
            <button type="submit">Delete</button>
        </form>
    </li>
</ul>
{% endblock %}
